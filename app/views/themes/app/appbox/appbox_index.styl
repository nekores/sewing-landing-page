//- APPBOX NAVBAR CSS
.appbox-nvbar
	.icon-bar
		background-color white
	@media(min-width 991px)
		.navbar-brand
			width 224px
			img
				top: -30px;
				height: 82px;
				// width: 243px;
	@media(min-width 767px) and (max-width 991px)
		.navbar-nav li a
			font-size 9px !important
			padding-left 8px !important
			padding-right 8px !important
		.navbar-brand
			width: 139px;
			img
				top: -30px;
				height: 69px;
				width: 243px;
	.navbar
		transition 0.5s all ease-in
		border none
		&.navbar-tranglo
			background-color white
			height 74px
			.navbar-nav
				margin-top: 19px;
				transition 0.5s all ease-in				
				li
					a
						font-size 14px
						color #969696
						padding-top: 7px;
						padding-bottom: 7px;
						padding-left 15px
						padding-right 15px
						border-radius 0
						&:link, &:visited
							letter-spacing 1px
							color #333
							text-transform uppercase
							font-size 12px
							transition 0.5s all
							border-radius 0
						&:hover, &:focus
							background-color transparent
							border-radius 0
							color #333
					&.active, a:visited
						border-radius 300px
						background-color primary
						a
							color white
			// brand customization
			.navbar-brand
				img
					top -42px
					// height 200px
					position relative	
			// Navbar header
			.navbar-header
				height 100%
				button
					border-color white
				.navbar-brand
					position relative
					top 25px
					transition 0.5s all ease-in

			&.affix
				background-color white
				transition 0.8s all
				height 50px
				border none
				box-shadow: 0 2px 5px rgba(0,0,0,0.26);
				
				.navbar-nav
					transition 0.8s all
					margin-top 8px
					font-size 9px !important
					li
						transition 0.8s all
						a
							color #404040
						&.active, a:visited
							a
								color white
				.navbar-header
					transition 0.8s all
					border none
					.navbar-brand
						transition 0.8s all
				.navbar-brand
					transition 0.8s all
					img
						transition 0.8s all
						top -48px
						height 55px
						position relative
			

			@media(max-width 767px)
				.container
					&, .col-xs-12, .row, [class ~="col-"], [class*=' col-'], .navbar-collapse
						padding 0
						margin 0
					.navbar-nav
						background-color primary
						li
							border-radius 0
					.navbar-header
						button
							border-color primary
							background-color primary
							border-color primary
				&.affix
					.navbar-header
						button
							border-color primary
							background-color primary
							border-color primary
							margin-top 8px
					.navbar-nav
						background-color #333
						li
							border-radius 0
							a
								background-color transparent
				.navbar-header
					border 0
					.navbar-brand
						display block
						margin 0 auto
						img
							// width: 200px;
							top: -48px;
					button
						margin-top 20px
						transition 0.5s all ease-in
				
				.navbar-nav
					background-color rgb(128, 128, 128)
					display block
					margin 0 !important
					width 100%
					padding 10px
					li
						a
							// background-color #8C272F
							color white !important
							&:hover
								background-color transparent
								border-radius 0
							&:focus
								background-color transparent
						&.active, a:visited
							border-left 1px solid white
							background-color transparent
							border-top 0
							border-right 0
							border-bottom 0

